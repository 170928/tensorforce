{
    "type": "dqn_agent",
    "update_mode": {
        "unit": "timesteps",
        "batch_size": 128,
        "frequency": 4
    },
    "memory": {
        "type": "replay",
        "capacity": 50000,
        "include_next_states": true
    },
    "optimizer": {
        "type": "clipped_step",
        "clipping_value": 0.005,
        "optimizer": {
            "type": "adam",
            "learning_rate": 5e-4
        }
    },

    "discount": 0.99,

    "target_sync_frequency": 500,
    "target_update_weight": 1.0,

    "double_q_model": true,

    "actions_exploration": {
        "type": "epsilon_anneal",
        "initial_epsilon": 0.8,
        "final_epsilon": 0.01,
        "timesteps": 10000
    }
}